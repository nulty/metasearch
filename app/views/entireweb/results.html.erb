<h1>Entireweb</h1>


<div>
	<%= paginate @items %>
	
	<table>
	<% @items.each do |item| %>
		<tr>
			<td>
				<span class="resTitle"><%= item["title"] %></span><br />
				<span class="resDesc"><%= item["snippet"] %><br /></span>
				<span class="resUrl"><%= link_to item["url"], item["Url"] %></span>
			</td>
		</tr>
		<% end %>
	</table>
</div>
