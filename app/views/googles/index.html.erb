<h1>Listing googles</h1>

<table>
  <tr>
    <th>Query</th>
    <th>Query rank</th>
    <th>Query number</th>
    <th>Title</th>
    <th>Description</th>
    <th>Url</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

  <%= paginate @googles%>
<% @googles.each do |google| %>
  <tr>
    <td><%= google.query %></td>
    <td><%= google.query_rank %></td>
    <td><%= google.query_number %></td>
    <td><%= google.title %></td>
    <td><%= google.description %></td>
    <td><%= google.url %></td>
    <td><%= link_to 'Show', google %></td>
    <td><%= link_to 'Edit', edit_google_path(google) %></td>
    <td><%= link_to 'Destroy', google, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Google', new_google_path %>
