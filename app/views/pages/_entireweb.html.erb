<h3>Entireweb Results</h3>

	<%= paginate @entireweb %>
	
	<table>
	<% @entireweb.each do |item| %>
		<tr>
			<td>
				<span class="resTitle"><%= item["Title"] %></span><br />
				<span class="resDesc"><%= item["Description"] %><br /></span>
				<span class="resUrl"><%=link_to truncate(item["Url"]), item["Url"] %></span>
			</td>
		</tr>
		<% end %>
	</table>

