<%= render 'search'%>

	<div class="query-panel">
	<ul>	
		<% @queries.each do |q|%>
			<li><%= link_to q.query, searchresults_path(:query=>q.query_number) %></li>
		<% end %>	
	</ul>
	</div>
	
	<% if @blekko %>
		<div id="blekkoDiv" class="result_div">
			<%= render 'blekko.html.erb'%>
		</div>
	<% end %>
	
	<% if @entireweb %>
		<div id="entireDiv" class="result_div">
			<%= render 'entireweb.html.erb'%>
		</div>
	<% end %>
	
	<% if @bing %>
		<div id="bingDiv" class="result_div">
			<%= render 'bing.html.erb'%>
		</div>
	<% end %>

